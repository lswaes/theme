			"fontSizes": [
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Section Title",
					"size": "clamp(36px,3vw,45px)",
					"slug": "slider-title"
				},
				{
					"name": "Normal",
					"size": "1rem",
					"slug": "normal"
				},
				{
					"name": "Medium",
					"size": "1.7rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(1.75rem, 3vw, 2.375rem)",
					"slug": "large"
				},
				{
					"name": "Huge",
					"size": "clamp(1.5625rem, 1.0625rem + 2.5vw, 3rem)",
					"slug": "huge"
				},
				{
					"name": "Gigantic",
					"size": "clamp(2rem, 6vw, 4rem)",
					"slug": "gigantic"
				},
